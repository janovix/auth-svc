{
	"compatibility_date": "2025-10-08",
	"compatibility_flags": ["nodejs_compat"],
	"main": "src/index.ts",
	"name": "auth-svc-test",
	"upload_source_maps": true,
	"workers_dev": true,
	"observability": {
		"enabled": true,
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "auth-test",
			"database_id": "test-database-id",
		},
	],
	"kv_namespaces": [
		{
			"binding": "KV",
			"id": "test-kv-id",
		},
	],
	// No service binding for tests - service will be mocked via serviceBindings in vitest config
	"vars": {
		"ENVIRONMENT": "test",
		"BETTER_AUTH_URL": "https://auth-svc.test.workers.dev",
		"AUTH_TRUSTED_ORIGINS": "https://*.janovix.workers.dev",
		"AUTH_FRONTEND_URL": "https://auth.janovix.workers.dev",
	},
}
